var agora = new Date()
var semana = agora.getDay()


switch(semana){
    case 0:
        console.log('DOMINGO')
        break
    case 1:
        console.log('SEGUNDA')    
        break
    case 2:
        console.log('TERÃ‡A ')    
        break
    case 3:
        console.log('QUARTA')
            break
    case 4:
        console.log('QUINTA')
        break
    case 5:
        console.log('SEXTA')
        break
    case 6:
        console.log('SABADO')
        break

}
